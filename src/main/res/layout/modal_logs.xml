<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:ntxdev="http://schemas.android.com/apk/res-auto"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#e5e5e5"
                android:padding="26dp">

    <com.lfdb.parapesquisa.ui.FontTextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="FORMULÃRIOS"
            android:id="@+id/textView"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:textColor="#676767"
            android:textSize="12dp"/>

    <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="70dp"
            android:layout_below="@+id/textView"
            android:layout_alignLeft="@+id/textView"
            android:background="#cccccc"
            android:layout_marginTop="10dp"
            android:padding="1dp"
            android:id="@+id/frameLayout"
            android:layout_marginBottom="24dp">

        <RelativeLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_gravity="center"
                android:background="#fff"
                android:visibility="visible"
                android:id="@+id/logs_myforms">

            <FrameLayout
                    android:layout_width="90dp"
                    android:layout_height="fill_parent"
                    android:layout_marginLeft="45dp">

                <com.lfdb.parapesquisa.ui.FontTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="0"
                        android:id="@+id/logs_formcount"
                        android:layout_centerVertical="true"
                        android:layout_alignParentLeft="false"
                        android:textSize="30dp"
                        android:layout_alignParentRight="false"
                        android:layout_gravity="center_vertical|right"
                        android:textColor="#333"/>
            </FrameLayout>

            <com.lfdb.parapesquisa.ui.FontTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/log_forms"
                    android:id="@+id/textView3"
                    android:layout_centerVertical="true"
                    android:layout_alignParentLeft="true"
                    android:layout_marginLeft="150dp"
                    android:textSize="17dp"
                    android:textColor="#333"
                    android:paddingTop="1dp"
                ntxdev:font_weight="300"/>

            <ImageView
                    android:layout_width="50dp"
                    android:layout_height="match_parent"
                    android:id="@+id/imageView"
                    android:src="@drawable/bt_seta"
                    android:layout_alignParentRight="true"
                    android:scaleType="center"/>

            <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/imageView2"
                    android:layout_centerVertical="true"
                    android:layout_alignParentLeft="true"
                    android:src="@drawable/relatorio_icon3"
                    android:layout_marginLeft="20dp"/>

        </RelativeLayout>
    </FrameLayout>

    <com.lfdb.parapesquisa.ui.FontTextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="PESQUISADORES"
            android:id="@+id/textView4"
            android:layout_below="@+id/frameLayout"
            android:layout_alignLeft="@+id/frameLayout"
            android:textSize="12dp"
            android:textColor="#676767"/>

    <FrameLayout
            android:layout_width="fill_parent"
            android:layout_height="70dp"
            android:layout_below="@+id/textView4"
            android:layout_alignLeft="@+id/textView4"
            android:background="#ccc"
            android:layout_marginTop="10dp"
            android:padding="1dp"
            android:layout_marginBottom="24dp"
            android:id="@+id/frameLayout3">

        <RelativeLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_gravity="left|top"
                android:background="#fff"
                android:visibility="visible"
                android:id="@+id/logs_myusers">

            <FrameLayout
                    android:layout_width="90dp"
                    android:layout_height="fill_parent"
                    android:layout_marginLeft="45dp">

                <com.lfdb.parapesquisa.ui.FontTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="0"
                        android:id="@+id/logs_usercount"
                        android:layout_centerVertical="true"
                        android:layout_alignParentLeft="false"
                        android:textSize="30dp"
                        android:layout_alignParentRight="false"
                        android:layout_gravity="center_vertical|right"
                        android:textColor="#333"/>
            </FrameLayout>

            <com.lfdb.parapesquisa.ui.FontTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/log_users"
                    android:id="@+id/logs_text_users"
                    android:layout_centerVertical="true"
                    android:layout_alignParentLeft="true"
                    android:layout_marginLeft="150dp"
                    android:textSize="17dp"
                    android:textColor="#333"
                    android:paddingTop="1dp"
                ntxdev:font_weight="300"/>

            <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/imageView3"
                    android:layout_centerVertical="true"
                    android:layout_alignParentLeft="true"
                    android:layout_marginLeft="15dp"
                    android:src="@drawable/relatorio_icon4"/>
        </RelativeLayout>
    </FrameLayout>

    <com.lfdb.parapesquisa.ui.FontTextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="PESQUISAS"
            android:id="@+id/textView7"
            android:textSize="12dp"
            android:textColor="#676767"
            android:layout_below="@+id/frameLayout3"
            android:layout_alignLeft="@+id/frameLayout3"/>

    <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:background="#ccc"
            android:id="@+id/frameLayout2"
            android:layout_below="@+id/textView7"
            android:layout_alignLeft="@+id/frameLayout3"
            android:layout_marginTop="10dp"
            android:padding="1dp"
            android:orientation="vertical">

        <RelativeLayout
                android:layout_width="fill_parent"
                android:layout_height="68dp"
                android:layout_gravity="left|top"
                android:background="#fff"
                android:visibility="visible"
                android:id="@+id/logs_hint_total"
                android:layout_marginBottom="1dp">

            <FrameLayout
                    android:layout_width="90dp"
                    android:layout_height="fill_parent"
                    android:layout_marginLeft="45dp">

                <com.lfdb.parapesquisa.ui.FontTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="0"
                        android:id="@+id/logs_quota"
                        android:layout_centerVertical="true"
                        android:layout_alignParentLeft="false"
                        android:textSize="30dp"
                        android:layout_alignParentRight="false"
                        android:layout_gravity="center_vertical|right"
                        android:textColor="#333"/>
            </FrameLayout>

            <com.lfdb.parapesquisa.ui.FontTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/log_submissions_total"
                    android:id="@+id/textView9"
                    android:layout_centerVertical="true"
                    android:layout_alignParentLeft="true"
                    android:layout_marginLeft="150dp"
                    android:textSize="17dp"
                    android:textColor="#333"
                    android:paddingTop="1dp"
                ntxdev:font_weight="300"/>

            <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/imageView4"
                    android:layout_centerVertical="true"
                    android:layout_alignParentLeft="true"
                    android:layout_marginLeft="15dp"
                    android:src="@drawable/relatorio_icon5"/>
        </RelativeLayout>

        <RelativeLayout
                android:layout_width="fill_parent"
                android:layout_height="68dp"
                android:layout_gravity="left|top"
                android:background="#fff"
                android:visibility="visible"
                android:id="@+id/logs_hint_remaining"
                android:layout_marginBottom="1dp">

            <FrameLayout
                    android:layout_width="90dp"
                    android:layout_height="fill_parent"
                    android:layout_marginLeft="45dp">

                <com.lfdb.parapesquisa.ui.FontTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="0"
                        android:id="@+id/logs_remaining"
                        android:layout_centerVertical="true"
                        android:layout_alignParentLeft="false"
                        android:textSize="30dp"
                        android:layout_alignParentRight="false"
                        android:layout_gravity="center_vertical|right"
                        android:textColor="#333"/>
            </FrameLayout>

            <com.lfdb.parapesquisa.ui.FontTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/log_submissions_remaining"
                    android:id="@+id/textView11"
                    android:layout_centerVertical="true"
                    android:layout_alignParentLeft="true"
                    android:layout_marginLeft="150dp"
                    android:textSize="17dp"
                    android:textColor="#333"
                    android:paddingTop="1dp"
                ntxdev:font_weight="300"/>

            <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/imageView5"
                    android:layout_centerVertical="true"
                    android:layout_alignParentLeft="true"
                    android:layout_marginLeft="15dp"
                    android:src="@drawable/relatorio_icon6"/>
        </RelativeLayout>

        <RelativeLayout
                android:layout_width="fill_parent"
                android:layout_height="68dp"
                android:layout_gravity="left|top"
                android:background="#fff"
                android:visibility="visible"
                android:id="@+id/logs_hint_approved"
                android:layout_marginBottom="1dp">

            <FrameLayout
                    android:layout_width="90dp"
                    android:layout_height="fill_parent"
                    android:layout_marginLeft="45dp">

                <com.lfdb.parapesquisa.ui.FontTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="0"
                        android:id="@+id/logs_approved"
                        android:layout_centerVertical="true"
                        android:layout_alignParentLeft="false"
                        android:textSize="30dp"
                        android:layout_alignParentRight="false"
                        android:layout_gravity="center_vertical|right"
                        android:textColor="#333"/>
            </FrameLayout>

            <com.lfdb.parapesquisa.ui.FontTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/log_submissions_approved"
                    android:id="@+id/textView13"
                    android:layout_centerVertical="true"
                    android:layout_alignParentLeft="true"
                    android:layout_marginLeft="150dp"
                    android:textSize="17dp"
                    android:textColor="#333"
                    android:paddingTop="1dp"
                ntxdev:font_weight="300"/>

            <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/imageView6"
                    android:layout_centerVertical="true"
                    android:layout_alignParentLeft="true"
                    android:src="@drawable/relatorio_icon7"
                    android:layout_marginLeft="15dp"/>
        </RelativeLayout>

        <RelativeLayout
                android:layout_width="fill_parent"
                android:layout_height="68dp"
                android:layout_gravity="left|top"
                android:background="#fff"
                android:visibility="visible"
                android:id="@+id/logs_hint_misc"
                >

            <FrameLayout
                    android:layout_width="90dp"
                    android:layout_height="fill_parent"
                    android:layout_marginLeft="45dp">

                <com.lfdb.parapesquisa.ui.FontTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="0"
                        android:id="@+id/logs_other"
                        android:layout_centerVertical="true"
                        android:layout_alignParentLeft="false"
                        android:textSize="30dp"
                        android:layout_alignParentRight="false"
                        android:layout_gravity="center_vertical|right"
                        android:textColor="#333"/>
            </FrameLayout>

            <com.lfdb.parapesquisa.ui.FontTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/log_submission_waiting"
                    android:id="@+id/textView15"
                    android:layout_centerVertical="true"
                    android:layout_alignParentLeft="true"
                    android:layout_marginLeft="150dp"
                    android:textSize="17dp"
                    android:textColor="#333"
                    android:paddingTop="1dp"
                ntxdev:font_weight="300"/>

            <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/imageView7"
                    android:layout_centerVertical="true"
                    android:layout_alignParentLeft="true"
                    android:src="@drawable/relatorio_icon8"
                    android:layout_marginLeft="15dp"/>
        </RelativeLayout>
    </LinearLayout>
</RelativeLayout>